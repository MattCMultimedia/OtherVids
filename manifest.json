{
  "name": "OtherVids",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Reenables the 'Other videos from this channel' feature from YouTube past.",
  "homepage_url": "http://matt.cond.in/OtherVids",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "*://*.youtube.com/watch*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "*://*.youtube.com/watch*"
      ],
      "js": [
        "src/inject/inject.js",
        "js/jquery/jquery.min.js",
        "js/underscore/underscore.min.js"
      ]
    }
  ]
}